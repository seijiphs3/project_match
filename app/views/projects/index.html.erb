<h1>Listing projects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Summary</th>
      <th>Tag1</th>
      <th>Tag2</th>
      <th>Tag3</th>
      <th>Tag4</th>
      <th>Tag5</th>
      <th>Tag6</th>
      <th>Tag7</th>
      <th>Tag8</th>
      <th>Tag9</th>
      <th>Tag10</th>
      <th>Place</th>
      <th>Url</th>
      <th>Start date</th>
      <th>Member</th>
      <th>Job</th>
      <th>Skill description</th>
      <th>Skill tag1</th>
      <th>Skill tag2</th>
      <th>Skill tag3</th>
      <th>Skill tag4</th>
      <th>Skill tag5</th>
      <th>Skill tag6</th>
      <th>Skill tag7</th>
      <th>Skill tag8</th>
      <th>Skill tag9</th>
      <th>Skill tag10</th>
      <th>Owner pic url</th>
      <th>Pic position</th>
      <th>Vision</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= project.summary %></td>
        <td><%= project.tag1 %></td>
        <td><%= project.tag2 %></td>
        <td><%= project.tag3 %></td>
        <td><%= project.tag4 %></td>
        <td><%= project.tag5 %></td>
        <td><%= project.tag6 %></td>
        <td><%= project.tag7 %></td>
        <td><%= project.tag8 %></td>
        <td><%= project.tag9 %></td>
        <td><%= project.tag10 %></td>
        <td><%= project.place %></td>
        <td><%= project.url %></td>
        <td><%= project.start_date %></td>
        <td><%= project.member %></td>
        <td><%= project.job %></td>
        <td><%= project.skill_description %></td>
        <td><%= project.skill_tag1 %></td>
        <td><%= project.skill_tag2 %></td>
        <td><%= project.skill_tag3 %></td>
        <td><%= project.skill_tag4 %></td>
        <td><%= project.skill_tag5 %></td>
        <td><%= project.skill_tag6 %></td>
        <td><%= project.skill_tag7 %></td>
        <td><%= project.skill_tag8 %></td>
        <td><%= project.skill_tag9 %></td>
        <td><%= project.skill_tag10 %></td>
        <td><%= project.owner_pic_url %></td>
        <td><%= project.pic_position %></td>
        <td><%= project.vision %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

